{
    "name": "helsingborg-stad/project-synchronizer",
    "description": "A utitlity to synchronize projects from one repo to another",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "phpunit --no-coverage --testdox"
    },
    "authors": [
        {
            "name": "Petter Andersson",
            "email": "petter.andersson@helsingborg.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "source/php/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "composer/semver": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "carthage-software/mago": "^0.26.1"
    },
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true
        }
    },
    "version": "0.1.16"
}